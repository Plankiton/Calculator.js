{
  "expo": {
    "name": "Calculator",
    "slug": "calculator",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#35495c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robocopgay.calculator"
    },
    "android":{
      "package": "com.robocopgay.calculator"
    },
    "description": "A beautiful calculator app for android",
    "githubUrl": "https://github.com/RoboCopGay/Calculator"
  }
}
